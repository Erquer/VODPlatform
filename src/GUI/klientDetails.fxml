<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<DialogPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ClientController">
   <content>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
          <Label text="ID:" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.valignment="TOP" />
          <TextField fx:id="id" GridPane.columnIndex="0" GridPane.rowIndex="0" />
          <Label text="Data Urodzenia:" GridPane.rowIndex="0" GridPane.columnIndex="1" GridPane.valignment="TOP"/>
          <TextField fx:id="data" GridPane.columnIndex="1" GridPane.rowIndex="0" />
          <Label text="Adres Email:" GridPane.rowIndex="0" GridPane.columnIndex="2" GridPane.valignment="TOP"/>
          <TextField fx:id="email" GridPane.columnIndex="2" GridPane.rowIndex="0" />
          <Label text="Numer karty:" GridPane.rowIndex="1" GridPane.columnIndex="0" GridPane.valignment="TOP"/>
          <TextField fx:id="card" GridPane.columnIndex="0" GridPane.rowIndex="1" />
          <Label text="Abonamenty:" GridPane.rowIndex="1" GridPane.columnIndex="1" GridPane.valignment="TOP"/>
          <TextField fx:id="abonament" GridPane.columnIndex="1" GridPane.rowIndex="1" />
          <Label text="Termin abonamentu:" GridPane.rowIndex="1" GridPane.columnIndex="2" GridPane.valignment="TOP"/>
          <TextField fx:id="terminAbo" GridPane.columnIndex="2" GridPane.rowIndex="1" />
          <ListView fx:id="produkcje" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="2" />
          <ListView fx:id="terminy" GridPane.columnIndex="2" GridPane.rowIndex="2" />
      </GridPane>
   </content>
</DialogPane>
